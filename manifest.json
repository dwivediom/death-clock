{
    "manifest_version": 3,
    "name": "100 Year Clock New Tab",
    "version": "1.0",
    "description": "A visual countdown clock showing time in multiple formats",
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "permissions": ["storage"],
    "icons": {
      "16": "icons/clock.png",
      "48": "icons/clock.png",
      "128": "icons/clock.png"
    }
  }